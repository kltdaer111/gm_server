<script type="text/javascript">

//重新渲染表单
 $(function () {
    //clear_localStorage();
    $(document).ready(function () {
      init_online_check_login_status();
       var list = reload_select_list();
        for(var i = 0; i < list.length; i++)
        {
          $("#zcySelect").append("<option value='"+list[i].id+"'>"+list[i].name+"</option>");
        }
        renderForm();
    });        
  });

function page(json){
    layui.use('table',function(){
        var table = layui.table;

        table.render({
            elem:"#record_table_base_info",
            height: 80,
            //width:'100%',
            cols: [[ //标题栏
                {field: 'server_id', title: '<font size="3" face="楷体">服务器ID</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'role_did', title: '<font size="3" face="楷体">角色DID</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'role_uid', title: '<font size="3" face="楷体">角色UID</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'role_name', title: '<font size="3" face="楷体">角色名</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'create_time', title: '<font size="3" face="楷体">创建时间</font>', align: 'center',style:'background-color: #f2f2f2;'}
                // ,{field: 'last_login_time', title: '<font size="3" face="楷体">最近登录时间</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'last_logout_time', title: '<font size="3" face="楷体">最近登出时间</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'level', title: '<font size="3" face="楷体">角色等级</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'fighting', title: '<font size="3" face="楷体">战斗力</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'scene_id', title: '<font size="3" face="楷体">所在场景</font>', align: 'center',style:'background-color: #f2f2f2;'}
        ]],
        data:json,
        page: false, //是否显示分页
        });

          table.render({
            elem:"#record_table_money",
            height: 80,
            //width:'100%',
            cols: [[ //标题栏
                {field: 'total_recharge', title: '<font size="3" face="楷体">充值金额</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'gold', title: '<font size="3" face="楷体">金币</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'silver', title: '<font size="3" face="楷体">银币</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'copper', title: '<font size="3" face="楷体">铜币</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'life_stone', title: '<font size="3" face="楷体">命石</font>', align: 'center',style:'background-color: #f2f2f2;'}
                ,{field: 'vip_level', title: '<font size="3" face="楷体">VIP等级</font>', align: 'center',style:'background-color: #f2f2f2;'}
        ]],
        data:json,
        page: false, //是否显示分页
        });
    });    
}


layui.use(['form', 'layedit','element','table'], function(){
  var form = layui.form
  ,layer = layui.layer
  ,layedit = layui.layedit
  ,laytab = layui.element
  ,table = layui.table;

form.verify({
  zcySelect: function(value){
      if (value == 0) {
        return "请选择服务器"
      }
    }
    , date_verify_min: function(value){
      var time_min = document.getElementById('logmin').value;
      var time_max = document.getElementById('logmax').value;
      if(time_max != ''){
        if (time_min == '') {
          return '请选择时间';
        }
      }
    }
    ,date_verify_max: function(value){
      var time_min = document.getElementById('logmin').value;
      var time_max = document.getElementById('logmax').value;
      if(time_min != ''){
          if(time_max == '') {
            return '请选择时间';
          }
      }
    }
  });


  form.on('submit(query)', function(data){
        var server_id_list = $('#zcySelect').val();
        var send = new Object();
        send.sid = new Array(server_id_list);
         if (data.field.role_did != "") {
          send.id = data.field.role_did;  
        } else if(data.field.role_name != ""){
          send.role_name = data.field.role_name;
        }
        var time_min = document.getElementById('logmin').value;
        var time_max = document.getElementById('logmax').value;
        if (time_min != '' && time_max != '') {
            send.start_time = get_msec_to_sec(time_min);
            send.end_time = get_msec_to_sec(time_max);
        }
        
        $.ajax({
          type: "POST",
          url: '../php/sg_gm/search_role.php',
          dataType: "json",
          data: {
            data: JSON.stringify(send)
          },
          success: function (json) {
              if (1 == json) {
                layer.msg("失败", { icon: 5, time: 2000 });
                page(json);
              } else {
                page(data_mod(json,'base_info'));
                layer.msg("成功", { icon: 6, time: 2000 });
              }
              table.render();
          },
          error: function (er) {
              console.log(er);
              renderForm();
              layer.msg("错误", { icon: 5, time: 2000 });
          },
        });
        return false;
    });
});
</script> 
