        //重新渲染表单
        $(function () {
            $(document).ready(function () {
                init_online_check_login_status();
                var list = reload_select_list();
                for (var i = 0; i < list.length; i++) {
                    $("#zcySelect").append("<option value='" + list[i].id + "'>" + list[i].name +
                        "</option>");
                }

                for (var i = 0; i < list.length; i++) {
                    $("#zcySelect_1").append("<option value='" + list[i].id + "'>" + list[i].name +
                        "</option>");
                }
                renderForm();
            });
        });


        function get_items() {
            var items = new String();
            for (var i = 1; i < 6; i++) {
                var it = '#item' + i;
                var nu = '#number' + i;
                var item = $(it).val();
                var number = $(nu).val();
                if (item != 0 && number != 0) {
                    items = items + item + ':' + number + '$';
                }
            }
            return items;
        }

        layui.use(['layer', 'form', 'layedit', 'laydate', 'element', 'table'], function () {
            var form = layui.form,
                layer = layui.layer,
                layedit = layui.layedit,
                laydate = layui.laydate,
                laytab = layui.element,
                table = layui.table;

            //自定义验证规则
            form.verify({
                zcySelect: function (value) {
                    if (value == 0) {
                        return "请选择服务器"
                    }
                },
                zcySelect_1: function (value) {
                    if (value == 0) {
                        return "请选择服务器"
                    }
                },
                role_uid: function (value) {
                    var val = $('input:radio[name="range"]:checked').val();
                    if (val == "Single") {
					console.log(value);
                        var is_number = regex_number(value);
						console.log(is_number);
                        if (is_number == null) {
                            return "请输入正确的玩家DID";
                        }
                    }
                },
                role_uid_1: function (value) {
                    if (value != "") {
                        var is_number = regex_number(value);
                        if (is_number == null) {
                            return "请输入正确的玩家DID";
                        }
                    }
                },
                title: function (value) {
                    if (value.length < 1) {
                        return '请至少得1个字符';
                    } else if (value.length > 50) {
                        return '长度超过50';
                    }
                },
                date_verify: function (value) {
                    var time = document.getElementById('log_time').value;
                    var val = $('input:radio[name="time"]:checked').val();
                    if (val == 'on_time') {
                        if (time == 0) {
                            return '请选择时间';
                        }
                    }
                },
                number_verify: function (value, item) {
                    if (value != "") {
                        if (0 == value) {
                            return '数值不能为0';
                        }
                        var is_number = regex_number(value);
                        if (is_number == null) {
                            return '请输入正确的数字';
                        }
                    }

                    switch (item.id) {
                        case 'item1':
                            var number1 = $('#number1').val();
                            console.log(number1, value);
                            if (number1 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;
                        case 'item2':
                            var number2 = $('#number2').val();
                            if (number2 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;

                        case 'item3':
                            var number3 = $('#number3').val();
                            if (number3 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;

                        case 'item4':
                            var number4 = $('#number4').val();
                            if (number4 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;

                        case 'item5':
                            var number5 = $('#number5').val();
                            if (number5 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;
                        default:
                            break;
                    }

                    switch (item.id) {
                        case 'number1':
                            var item1 = $('#item1').val();
                            if (item1 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;
                        case 'number2':
                            var item2 = $('#item2').val();
                            if (item2 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;

                        case 'number3':
                            var item3 = $('#item3').val();
                            if (item3 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;
                        case 'number4':
                            var item4 = $('#item4').val();
                            if (item4 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;
                        case 'number5':
                            var item5 = $('#item5').val();
                            if (item5 != 0) {
                                if (value == 0) {
                                    return '道具和数量必须同有数值或者同没有数值'
                                }
                            }
                            break;
                        default:
                            break;
                    }
                },
                date_verify_min: function (value) {
                    var time_max = document.getElementById('logmax').value;
                    if (time_max != '') {
                        if (value == '') {
                            return '请选择时间';
                        }
                    }
                },
                date_verify_max: function (value) {
                    var time_min = document.getElementById('logmin').value;
                    if (time_min != '') {
                        if (value == '') {
                            return '请选择时间';
                        }
                    }
                },
                role_uid_1: function (value) {
                    if (value != "") {
                        var is_number = regex_number(value);
                        if (is_number == null) {
                            return "请输入正确的玩家UID";
                        }
                    }
                }
            });
            
            //监听radio
            form.on('radio()', function(data){
                console.log(data);
                if(data.value === "All"){
                    //TODO 开启定时
                    document.getElementById('on_time').removeAttribute('disabled');
                }
                else if(data.value === "Single"){
                    //TODO 关闭定时
                    //document.getElementById('on_time').removeAttribute('checked');
                    // document.getElementById('in_time').setAttribute('checked', '1');
                    document.getElementById('on_time').setAttribute('disabled', '1');
                }
                else if(data.value == "on_time"){
