            //监听提交
            form.on('submit(send)', function (data) {
                console.log(data.field);
                var storage = window.localStorage;
                if (storage.account == "" || storage.account == undefined) {
                    layer.msg("没有登录操作无效", {
                        icon: 5,
                        time: 2000
                    });
                    return false;
                }
                var server_id_list = $('#zcySelect').val();
